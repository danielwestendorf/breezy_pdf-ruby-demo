    <script src="/reports/lib/jquery-flot/jquery.flot.js" type="text/javascript"></script>
    <script src="/reports/lib/jquery-flot/jquery.flot.pie.js" type="text/javascript"></script>
    <script src="/reports/lib/jquery-flot/jquery.flot.time.js" type="text/javascript"></script>
    <script src="/reports/lib/jquery-flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="/reports/lib/jquery-flot/plugins/curvedLines.js" type="text/javascript"></script>
    <script src="/reports/lib/jquery-flot/plugins/jquery.flot.tooltip.js" type="text/javascript"></script>
    <script src="/reports/lib/countup/countUp.min.js" type="text/javascript"></script>
    <script src="/reports/js/app-dashboard.js" type="text/javascript"></script>
    <script type="text/javascript">
      $(document).ready(function(){
        //initialize the javascript
        window.pdf = <%= !params[:pdf].nil? %>;
        App.init();
        App.dashboard();
        setTimeout(function() {
          window.pdfRenderReady = true
        }, 250)

      });
    </script>
